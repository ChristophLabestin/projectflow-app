{
  "firestore": {
    "database": "(default)",
    "location": "eur3",
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/editImageWithVertexAI",
        "function": "editImageWithVertexAI",
        "region": "europe-west3"
      },
      {
        "source": "/generatePasskeyRegistrationOptions",
        "function": "generatePasskeyRegistrationOptions",
        "region": "europe-west3"
      },
      {
        "source": "/verifyPasskeyRegistration",
        "function": "verifyPasskeyRegistration",
        "region": "europe-west3"
      },
      {
        "source": "/generatePasskeyAuthenticationOptions",
        "function": "generatePasskeyAuthenticationOptions",
        "region": "europe-west3"
      },
      {
        "source": "/verifyPasskeyAuthentication",
        "function": "verifyPasskeyAuthentication",
        "region": "europe-west3"
      },
      {
        "source": "/testSMTPConnection",
        "function": "testSMTPConnection",
        "region": "europe-west3"
      },
      {
        "source": "/sendContactFormEmail",
        "function": "sendContactFormEmail",
        "region": "europe-west3"
      },
      {
        "source": "/createBlogPost**",
        "function": "createBlogPost",
        "region": "europe-west3"
      },
      {
        "source": "/createBlogPost/**",
        "function": "createBlogPost",
        "region": "europe-west3"
      },
      {
        "source": "/newsletterSubscribe",
        "function": "newsletterSubscribe",
        "region": "europe-west3"
      },
      {
        "source": "/newsletterUnsubscribe",
        "function": "newsletterUnsubscribe",
        "region": "europe-west3"
      },
      {
        "source": "/getBlogPost",
        "function": "getBlogPosts",
        "region": "europe-west3"
      },
      {
        "source": "/getBlogPosts",
        "function": "getBlogPosts",
        "region": "europe-west3"
      },
      {
        "source": "/getCategories",
        "function": "getCategories",
        "region": "europe-west3"
      },
      {
        "source": "/manageCategories**",
        "function": "manageCategories",
        "region": "europe-west3"
      },
      {
        "source": "/manageCategories/**",
        "function": "manageCategories",
        "region": "europe-west3"
      },
      {
        "source": "/requestWaitlist",
        "function": "requestWaitlist",
        "region": "europe-west3"
      },
      {
        "source": "/confirmWaitlist",
        "function": "confirmWaitlist",
        "region": "europe-west3"
      },
      {
        "source": "/api/requestWaitlist",
        "function": "requestWaitlist",
        "region": "europe-west3"
      },
      {
        "source": "/api/confirmWaitlist",
        "function": "confirmWaitlist",
        "region": "europe-west3"
      },
      {
        "source": "/api/confirmWaitlist",
        "function": "confirmWaitlist",
        "region": "europe-west3"
      },
      {
        "source": "/requestNewsletterSignup",
        "function": "requestNewsletterSignup",
        "region": "europe-west3"
      },
      {
        "source": "/confirmNewsletterSignup",
        "function": "confirmNewsletterSignup",
        "region": "europe-west3"
      },
      {
        "source": "/api/requestNewsletterSignup",
        "function": "requestNewsletterSignup",
        "region": "europe-west3"
      },
      {
        "source": "/api/confirmNewsletterSignup",
        "function": "confirmNewsletterSignup",
        "region": "europe-west3"
      },
      {
        "source": "/tiktokCallback",
        "function": "tiktokCallback",
        "region": "europe-west3"
      },
      {
        "source": "/api/**",
        "function": "api",
        "region": "europe-west3"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "storage": {
    "rules": "storage.rules"
  },
  "functions": {
    "predeploy": [
      "npm --prefix \"$RESOURCE_DIR\" run build"
    ]
  }
}